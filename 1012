#include <bits/stdc++.h>
using namespace std;

int main() {
  double A,B,C,Triangle_A,Circle_A,Trapezium_A,Square_A,Rectangle_A;

  cin>> fixed >> setprecision(1) >> A >> B >> C;
  
  Triangle_A = 0.5 * A * C;
  Circle_A = 3.14159 * C * C;
  Trapezium_A = 0.5 * (A + B) * C;
  Square_A = B * B;
  Rectangle_A = A * B;

  cout << fixed << setprecision(3);
  cout<< "TRIANGULO: "<< Triangle_A <<"\n";
  cout<< "CIRCULO: "<< Circle_A <<"\n";
  cout<< "TRAPEZIO: "<< Trapezium_A <<"\n";
  cout<< "QUADRADO: "<< Square_A <<"\n";
  cout<< "RETANGULO: "<< Rectangle_A <<"\n";

return 0;
}
